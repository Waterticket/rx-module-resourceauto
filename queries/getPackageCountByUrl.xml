<query id="getPackageCountByUrl" action="select">
    <tables>
        <table name="resourceauto_update" />
    </tables>
    <columns>
        <column name="count(*)" alias="count" />
    </columns>
    <conditions>
        <condition operation="equal" column="repo_url" var="repo_url" notnull="notnull" />
    </conditions>
</query>
